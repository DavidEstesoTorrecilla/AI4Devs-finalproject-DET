# Backend Orchestrator - Variables de entorno
# Copiar este archivo a .env y configurar los valores

# Servidor XMPP para comunicación entre agentes
# Puede ser un servidor local (ejabberd, prosody) o uno público
XMPP_SERVER=localhost
XMPP_PASSWORD=password123

# URL del Gateway para comunicación HTTP
GATEWAY_URL=http://localhost:8000

# URL del servicio SurfSense para enriquecimiento de leads
SURFSENSE_URL=http://localhost:8002

# Redis Cache
REDIS_URL=redis://:organis_redis_password@localhost:6379/1
REDIS_TTL=7200  # Tiempo de expiración en segundos (2 horas)

# Puerto del Orchestrator
PORT=8001

# Nivel de logging
LOG_LEVEL=INFO
